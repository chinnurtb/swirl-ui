<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Swirl Admin</title>

    <link href="assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="assets/css/swirl.css" rel="stylesheet">
  </head>

  <body>
    <div class="container">
      <div class="col-md-12">
        <div class="section">
          <h2>Mappers</h2>
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Flow Id</th>
              <th>Pid</th>
              <th>Flow Module</th>
              <th>Stream Filter</th>
              <th>Reducer Node</th>
            </tr>
          </thead>
          <tbody>
          {% for flow_id, pid, flow_mod, stream_filter, reducer_node in mappers %}
            <tr>
              <td>{{ flow_id }}</td>
              <td>{{ pid }}</td>
              <td>{{ flow_mod }}</td>
              <td>{{ stream_filter }}</td>
              <td>{{ reducer_node }}</td>
            </tr>
          {% endfor %}
          </tbody>
        </table>
      </div>

      <div class="col-md-12">
        <div class="section">
          <h2>Reducers</h2>
        </div>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Flow Id</th>
              <th>Pid</th>
              <th>Flow Module</th>
              <th>Stream Filter</th>
              <th>Mapper Nodes</th>
            </tr>
          </thead>
          <tbody>
          {% for flow_id, pid, flow_mod, stream_filter, mapper_nodes in reducers %}
            <tr>
              <td>{{ flow_id }}</td>
              <td>{{ pid }}</td>
              <td>{{ flow_mod }}</td>
              <td>{{ stream_filter }}</td>
              <td>{{ mapper_nodes }}</td>
            </tr>
          {% endfor %}
          </tbody>
        </table>
      <div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
  </body>
</html>
