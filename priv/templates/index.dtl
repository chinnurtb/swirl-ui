<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Swirl Admin</title>

    <link href="assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="assets/css/swirl.css" rel="stylesheet">
  </head>

  <body>
    <div class="container">
      <div class="col-md-12">
        <div class="header">
          <h2 class="text-muted">Swirl Admin</h2>
        </div>
      </div>

      <div class="col-md-12">
        <div class="section">
          <h3>Mappers <small>({{ mappers_count }})</small></h3>
        </div>
        {% ifequal mappers_count 0 %}
          No mappers
        {% else %}
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Pid</th>
              <th>Flow Id</th>
              <th>Flow Module</th>
              <th>Stream Name</th>
              <th>Stream Filter</th>
              <th>Flush</th>
              <th>Reducer Node</th>
            </tr>
          </thead>
          <tbody>
          {% for pid, flow_id, flow_mod, stream_name, stream_filter, flush, reducer_node in mappers %}
            <tr>
              <td>{{ pid }}</td>
              <td>{{ flow_id }}</td>
              <td>{{ flow_mod }}</td>
              <td>{{ stream_name }}</td>
              <td>{{ stream_filter }}</td>
              <td>{{ flush }}</td>
              <td>{{ reducer_node }}</td>
            </tr>
          {% endfor %}
          </tbody>
        </table>
        {% endifequal %}
      </div>

      <div class="col-md-12">
        <div class="section">
          <h3>Reducers <small>({{ reducers_count }})</small></h3>
        </div>
        {% ifequal reducers_count 0 %}
          No reducers
        {% else %}
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Pid</th>
              <th>Flow Id</th>
              <th>Flow Module</th>
              <th>Stream Name</th>
              <th>Stream Filter</th>
              <th>Flush</th>
              <th>Mapper Nodes</th>
            </tr>
          </thead>
          <tbody>
          {% for pid, flow_id, flow_mod, stream_name, stream_filter, flush, mapper_nodes in reducers %}
            <tr>
              <td>{{ pid }}</td>
              <td>{{ flow_id }}</td>
              <td>{{ flow_mod }}</td>
              <td>{{ stream_name }}</td>
              <td>{{ stream_filter }}</td>
              <td>{{ flush }}</td>
              <td>{{ mapper_nodes }}</td>
            </tr>
          {% endfor %}
          </tbody>
        </table>
        {% endifequal %}
      <div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
  </body>
</html>
